{
  "name": "@componentor/quickjs-emscripten",
  "version": "0.31.24",
  "sideEffects": false,
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "eval",
    "quickjs",
    "vm",
    "interpreter",
    "runtime",
    "safe",
    "emscripten",
    "wasm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/componentor/quickjs-emscripten"
  },
  "author": {
    "name": "Componentor"
  },
  "files": [
    "LICENSE",
    "dist/**/*",
    "!dist/*.test.js",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "npx tsup",
    "clean": "git clean -fx dist .output",
    "tsc": "npx tsc -p . --noEmit",
    "test:vi": "VITEST=1 npx vitest run",
    "test:node": "npx tsc -p tsconfig.build.json && ./makeTestPackageJson.ts && node --experimental-import-meta-resolve -r source-map-support/register --test ./.output/*node-test*.*js",
    "test": "npm run test:vi && npm run test:node"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./variants": {
      "types": "./dist/variants.d.ts",
      "import": "./dist/variants.mjs",
      "require": "./dist/variants.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@componentor/quickjs-wasmfile-debug-asyncify": "workspace:*",
    "@componentor/quickjs-wasmfile-debug-sync": "workspace:*",
    "@componentor/quickjs-wasmfile-release-asyncify": "workspace:*",
    "@componentor/quickjs-wasmfile-release-sync": "workspace:*",
    "@componentor/quickjs-wasmfs-release-sync": "workspace:*",
    "@componentor/quickjs-emscripten-core": "workspace:*"
  },
  "devDependencies": {
    "@componentor/quickjs-ffi-types": "workspace:*",
    "@componentor/quickjs-asmjs-mjs-release-sync": "workspace:*",
    "@componentor/quickjs-ng-wasmfile-debug-asyncify": "workspace:*",
    "@componentor/quickjs-ng-wasmfile-debug-sync": "workspace:*",
    "@componentor/quickjs-ng-wasmfile-release-asyncify": "workspace:*",
    "@componentor/quickjs-ng-wasmfile-release-sync": "workspace:*",
    "@componentor/quickjs-singlefile-cjs-debug-asyncify": "workspace:*",
    "@componentor/quickjs-singlefile-cjs-debug-sync": "workspace:*",
    "@componentor/quickjs-singlefile-cjs-release-asyncify": "workspace:*",
    "@componentor/quickjs-singlefile-cjs-release-sync": "workspace:*",
    "@componentor/quickjs-singlefile-mjs-debug-asyncify": "workspace:*",
    "@componentor/quickjs-singlefile-mjs-debug-sync": "workspace:*",
    "@componentor/quickjs-singlefile-mjs-release-asyncify": "workspace:*",
    "@componentor/quickjs-singlefile-mjs-release-sync": "workspace:*",
    "@jitl/tsconfig": "workspace:*"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
